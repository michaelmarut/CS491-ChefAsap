{
  "expo": {
    "name": "ChefAsap",
    "slug": "ChefAsap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/chefAsapLogo1Circle.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ChefAsap needs access to your location to find nearby chefs."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
